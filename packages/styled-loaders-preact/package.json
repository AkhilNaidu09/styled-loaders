{
  "name": "styled-loaders-preact",
  "version": "0.1.0",
  "main": "index.js",
  "license": "MIT",
  "author": "Sara Vieira",
  "scripts": {
    "start": "if-env NODE_ENV=production && yarn run -s serve || yarn run -s dev",
    "build": "preact build",
    "serve": "preact build && preact serve",
    "dev": "preact watch",
    "test": "BABEL_ENV=test jest --no-cache",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "devDependencies": {
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "eslint": "^4.4.1",
    "eslint-config-synacor": "^1.1.0",
    "if-env": "^1.0.0",
    "jest": "^20.0.4",
    "jest-serializer-html-string": "^1.0.1",
    "jest-styled-components": "^4.4.0",
    "preact-cli": "^1.4.1",
    "preact-render-to-string": "^3.6.3"
  },
  "dependencies": {
    "preact": "^8.2.1",
    "preact-compat": "^3.17.0",
    "preact-router": "^2.5.5",
    "prop-types": "^15.5.10",
    "styled-components": "^2.1.2"
  },
  "jest": {
    "testEnvironment": "node",
    "snapshotSerializers": [
      "jest-serializer-html-string"
    ],
    "collectCoverage": true,
    "coverageDirectory": ".coverage"
  }
}
